<UserControl x:Class="Clouder.SubViews.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*" />
            <RowDefinition Height="3*" />
            <RowDefinition Height="3*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="3*" />
        </Grid.ColumnDefinitions>

        <Grid Grid.Row="0" Grid.Column="0" Background="Aqua">
            
        </Grid>
        <Grid Grid.Row="0" Grid.Column="1" Background="Azure">
            <TextBlock Text=" DASHBOARD " />
        </Grid>
        <Grid Grid.Row="0" Grid.Column="2" Background="AntiqueWhite"></Grid>
        <Grid Grid.Row="1" Grid.Column="0" Background="Beige"></Grid>
        <Grid Grid.Row="1" Grid.Column="1" Background="DarkViolet"></Grid>
        <Grid Grid.Row="1" Grid.Column="2" Background="DodgerBlue"></Grid>
        <Grid Grid.Row="2" Grid.Column="0" Background="Turquoise"></Grid>
        <Grid Grid.Row="2" Grid.Column="1" Background="LightCyan"></Grid>
        <Grid Grid.Row="2" Grid.Column="2" Background="AntiqueWhite"></Grid>
        
    </Grid>
</UserControl>
